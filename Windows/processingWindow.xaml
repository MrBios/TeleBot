        <Label Content="Задача" HorizontalAlignment="Center"/>
        <Label x:Name="name" Content="Задача х" HorizontalAlignment="Center"/>
        <Label Content="Выполняется..." HorizontalAlignment="Center"/>
    </StackPanel>
</Window>
